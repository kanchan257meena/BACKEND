<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>User</title>
    <style>
        table,th,td{
            border: 1px solid black;
            color: black;
        }
    </style>
</head>

<body>
    <table>
        <tr>
            <th>id</th>
            <th>email</th>
            <th>username</th>
        </tr>
       <% for(users of data){ %>
    <tr>
        <td><%= users.id %></td>
        <td><%= users.email %></td>
        <td><%= users.user %></td>
        <td>
            <form method="GET" action="/user/<%=users.id%>/edit">
            <button>Edit user</button>
        </form></td>
    </tr>
<% } %>

    </table>
</body>
</html>

